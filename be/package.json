{
  "name": "server",
  "version": "1.0.0",
  "main": "main.ts",
  "scripts": {
    "start": "node dist/main.js",
    "dev": "nodemon main.ts",
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prisma:merge": "node script/merge-prisma.mjs",
    "prisma:generate": "prisma generate --schema=src/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=src/prisma/schema.prisma --name init",
    "prisma:studio": "prisma studio --schema=src/prisma/schema.prisma",
    "prisma:pull": "npx dotenv -e .env.dev -- npx prisma db pull --schema=src/prisma/schema.prisma",
    "prisma:split": "node script/split-prisma.mjs",
    "generate-api": "npx openapi-typescript ./openapi.yaml --output ./src/types/api-contract.ts"
  },
  "keywords": [],
